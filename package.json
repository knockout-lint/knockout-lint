{
	"name": "knockout-lint",
	"description": "Lint and type check knockout.js bindings in the view with view model types.",
	"version": "0.0.2",
	"main": "./build/index.js",
	"types": "./build/index",
	"bin": "./build/cli/cli.js",
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.11.8",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"eslint": "^7.11.0",
		"jison": "^0.4.18",
		"mocha": "^8.1.3",
		"ts-node": "^9.0.0"
	},
	"dependencies": {
		"glob": "^7.1.6",
		"knockout": "^3.5.1",
		"meriyah": "^3.1.6",
		"source-map": "^0.7.3",
		"typescript": "^4.1.1-rc"
	},
	"scripts": {
		"build": "npx tsc --build . spec demo && npm run compile-parser",
		"watch": "npx tsc --build . spec demo --watch",
		"test": "node ./spec/build/spec.js",
		"demo": "node ./demo/build/lint.test.js",
		"lint": "eslint src/**/*.ts spec/src/**/*.ts",
		"compile-parser": "node build/parser/parser-compiler.js"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/knockout-lint/knockout-lint.git"
	},
	"keywords": [
		"ko",
		"knockout",
		"lint",
		"linter"
	],
	"author": "Elias Skogevall",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/knockout-lint/knockout-lint/issues"
	},
	"homepage": "https://github.com/knockout-lint/knockout-lint#readme"
}
