{
	"name": "knockout-lint",
	"description": "Lint and type check knockout.js bindings in the view with view model types.",
	"version": "0.1.0",
	"main": "./build/index.js",
	"types": "./build/index",
	"bin": {
		"kolint": "./bin/kolint.js",
		"knockout-lint": "./bin/knockout-lint.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/js-yaml": "^4.0.1",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.17.0",
		"@types/source-map": "^0.5.7",
		"@types/yargs": "^15.0.13",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.24.0",
		"eslint": "^7.26.0",
		"jison": "^0.4.18",
		"mocha": "^8.4.0",
		"ts-node": "^9.0.0",
		"typescript-json-schema": "^0.48.0"
	},
	"dependencies": {
		"js-yaml": "^4.1.0",
		"knockout": "^3.5.1",
		"meriyah": "^3.1.6",
		"source-map": "^0.7.3",
		"strip-json-comments": "^3.1.1",
		"tiny-glob": "^0.2.9",
		"typescript": "^4.2.4",
		"yargs": "^17.0.1"
	},
	"scripts": {
		"build": "npx tsc --build . spec && node build/parser/parser-compiler.js",
		"watch": "npx tsc --build . spec --watch",
		"test": "node ./spec/build/spec.js",
		"lint": "npx eslint src/**/*.ts spec/src/**/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kolint/kolint.git"
	},
	"keywords": [
		"ko",
		"knockout",
		"lint",
		"linter"
	],
	"author": "Elias Skogevall",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kolint/kolint/issues"
	},
	"homepage": "https://github.com/kolint/kolint#readme"
}
