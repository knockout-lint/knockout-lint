{
	"name": "knockout-lint",
	"description": "Lint and type check knockout.js bindings in the view with view model types.",
	"version": "0.0.4",
	"main": "./build/index.js",
	"types": "./build/index",
	"bin": {
		"kolint": "./build/cli/index.js",
		"knockout-lint": "./build/cli/index.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/js-yaml": "^4.0.1",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.14.44",
		"@types/source-map": "^0.5.7",
		"@types/yargs": "^15.0.13",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"eslint": "^7.26.0",
		"jison": "^0.4.18",
		"mocha": "^8.4.0",
		"ts-node": "^9.0.0",
		"typescript-json-schema": "^0.48.0"
	},
	"dependencies": {
		"js-yaml": "^4.1.0",
		"knockout": "^3.5.1",
		"meriyah": "^3.1.6",
		"source-map": "^0.7.3",
		"strip-json-comments": "^3.1.1",
		"tiny-glob": "^0.2.8",
		"typescript": "^4.2.4",
		"yargs": "^16.1.1"
	},
	"scripts": {
		"install": "node tools/gen-config-schema.js --no-docs --no-override",
		"build": "npx tsc --build . spec && npm run compile-parser",
		"watch": "npx tsc --build . spec --watch",
		"test": "node ./spec/build/spec.js",
		"lint": "npx eslint src/**/*.ts spec/src/**/*.ts",
		"compile-parser": "node build/parser/parser-compiler.js",
		"gen-config-schema": "node tools/gen-config-schema.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kolint/kolint.git"
	},
	"keywords": [
		"ko",
		"knockout",
		"lint",
		"linter"
	],
	"author": "Elias Skogevall",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kolint/kolint/issues"
	},
	"homepage": "https://github.com/kolint/kolint#readme"
}
