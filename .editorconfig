[*]
charset = utf-8
end_of_line = unset
indent_size = 3
indent_style = tab
insert_final_newline = false
root = true
tab_width = 3
trim_trailing_whitespace = true

[*.jison]
indent_size = 2
indent_style = space
tab_width = 2

[*.md]
insert_final_newline = true
indent_size = 2
indent_style = space
tab_width = 2

[*{.yaml,.yml}]
indent_size = 2
indent_style = space
